{
  "prefix": {
    "aida": "https://tac.nist.gov/tracks/SM-KBP/2018/ontologies/InterchangeOntology#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "xij": "http://isi.edu/xij-rule-set#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "ldcOnt": "https://tac.nist.gov/tracks/SM-KBP/2018/ontologies/SeedlingOntology#"
  },
  "class": {
    "Vehicle": {
      "path": "ldcOnt:Vehicle"
    },
    "Organization": {
      "path": "ldcOnt:Organization"
    },
    "Person": {
      "path": "ldcOnt:Person"
    },
    "Place": {
      "path": "ldcOnt:Place"
    },
    "Entity": {
      "path": "aida:Entity"
    },
    "text_entrypoint": {
      "path": "aida:TextJustification"
    },
    "video_entrypoint": {
      "path": "aida:VideoJustification"
    },
    "image_entrypoint": {
      "path": "aida:ImageJustification"
    },
    "audio_entrypoint": {
      "path": "aida:AudioJustification"
    }
  },
  "predicate": {
    "Conflict.Attack_Place": {
      "path": ["ldcOnt:conflict_attack_place"]
    },
    "Conflict.Attack_Instrument": {
      "path": ["ldcOnt:conflict_attack_instrument"]
    },
    "Conflict.Demonstrate_Place": {
      "path": ["ldcOnt:conflict_demonstrate_place"]
    },
    "Conflict.Demonstrate_Demonstrator": {
      "path": ["ldcOnt:conflict_demonstrate_demonstrator"]
    },
    "Conflict.Attack_Target": {
      "path": ["ldcOnt:conflict_attack_target"]
    },
    "Conflict.Attack_Attacker": {
      "path": ["ldcOnt:conflict_attack_attacker"]
    },
    "GeneralAffiliation.APORA_Affiliate": {
      "path": ["ldcOnt:affiliate"]
    },
    "Conflict.Attack_Agent": {
      "path": ["ldcOnt:conflict_attack_agent"]
    },
    "GeneralAffiliation.APORA_Affiliation": {
      "path": ["ldcOnt:affiliation"]
    },
    "Organization-Affiliation.Employment-Membership": {
      "path": ["ldcOnt:orgafl_empmem"]
    },
    "Physical.Located-Near": {
      "path": ["ldcOnt:phys_locnear"]
    },
    "General-Affiliation.Person-MORE": {
      "path": ["ldcOnt:genafl_more"]
    },
    "Contact.Meet_Participant": {
      "path": ["ldcOnt:contact_meet_participant"]
    },
    "Life.Die_Place": {
      "path": ["ldcOnt:life_die_place"]
    },
    "enttype": {
      "path": ["rdf:type"],
      "range": "uri"
    },
    "name_string": {
      "path": ["aida:hasName"],
      "range": "string"
    },
    "doceid": {
      "path": ["aida:justifiedBy", "aida:source"],
      "range": "string"
    },
    "start": {
      "path": ["aida:justifiedBy", "aida:startOffset"],
      "range": "number"
    },
    "end": {
      "path": ["aida:justifiedBy", "aida:endOffsetInclusive"],
      "range": "number"
    },
    "keyframeid": {
      "path": ["aida:justifiedBy", "aida:keyFrame"],
      "range": "string"
    },
    "topleft": {
      "splitter": ",",
      "split_to": [
        {
          "path": ["aida:justifiedBy", "aida:BoundingBoxPropertyShape", "aida:boundingBoxUpperLeftX"],
          "range": "number"
        },
        {
          "path": ["aida:justifiedBy", "aida:BoundingBoxPropertyShape", "aida:boundingBoxUpperLeftY"],
          "range": "number"
        }
      ]
    },
    "bottomright": {
      "splitter": ",",
      "split_to": [
        {
          "path": ["aida:justifiedBy", "aida:BoundingBoxPropertyShape", "aida:boundingBoxLowerRightX"],
          "range": "number"
        },
        {
          "path": ["aida:justifiedBy", "aida:BoundingBoxPropertyShape", "aida:boundingBoxLowerRightY"],
          "range": "number"
        }
      ]
    }
  }
}
